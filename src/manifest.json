{
  "name": "Lokika Homepage",
  "description": "Pretty startpage for you",
  "version": "1.0",
  "manifest_version": 3,
  "homepage_url": "https://github.com/lanvalird/lokika",
  "action": {
    "default_popup": "static/settings.html"
  },
  "options_page": "static/settings.html",
  "chrome_url_overrides": {
    "newtab": "static/newtab.html"
  },
  "permissions": ["search", "storage", "file://*/*"],
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  }
}
